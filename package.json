{
  "name": "olasearch-boilerplate",
  "version": "1.0.0",
  "description": "Ola Search starter",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "config": "cross-env NODE_ENV=production webpack --config webpack.config.demo.js --json | webpack-bundle-size-analyzer",
    "main": "cross-env NODE_ENV=production webpack --config webpack.config.main.js --json | webpack-bundle-size-analyzer",
    "demo": "npm run config && npm run main",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/olasearch/olasearch-project-starter.git"
  },
  "keywords": [],
  "author": "Vinay M <vinay@olasearch.com> (http://gitlab.com/OlaSearch)",
  "license": "Copyright Ola Search",
  "bugs": {
    "url": "https://gitlab.com/olasearch/olasearch-project-starterissues"
  },
  "homepage": "https://gitlab.com/olasearch/olasearch-project-starter",
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-olasearch": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "cross-env": "^2.0.1",
    "eslint": "^3.4.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-react": "^6.2.0",
    "eventsource-polyfill": "^0.9.6",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "rimraf": "^2.5.4",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "olasearch": "git+ssh://git@bitbucket.org/olasearch/olasearch-core.git",
    "olasearch-elasticsearch-adapter": "git+ssh://git@bitbucket.org/olasearch/elasticsearch-adapter.git",
    "olasearch-solr-adapter": "git+ssh://git@bitbucket.org/olasearch/solr-adapter.git",
    "olasearch-logger-middleware": "git+ssh://git@bitbucket.org/olasearch/olasearch-logger-middleware.git",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
