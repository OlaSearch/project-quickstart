{
  "name": "olasearch-boilerplate",
  "version": "1.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf dist",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "config": "cross-env NODE_ENV=production webpack --config webpack.config.demo.js",
    "main": "cross-env NODE_ENV=production webpack --config webpack.config.main.js --json | webpack-bundle-size-analyzer",
    "demo": "npm run config && npm run main",
    "build": "npm run clean && npm run build:webpack",
    "start": "node devServer.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-transform-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "author": "Vinay M <vinay@olasearch.com> (http://github.com/OlaSearch)",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/gaearon/react-transform-boilerplate/issues"
  },
  "homepage": "https://github.com/gaearon/react-transform-boilerplate",
  "devDependencies": {
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.0",
    "cross-env": "^1.0.6",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "eventsource-polyfill": "^0.9.6",
    "expose-loader": "^0.7.1",
    "express": "^4.13.3",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "olasearch": "git+ssh://git@bitbucket.org:olasearch/olasearch-core.git",
    "olasearch-elasticsearch-adapter": "^0.0.4",
    "olasearch-solr-adapter": "^0.0.18",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.3",
    "c3": "git+https://github.com/masayuki0812/c3.git",
    "classnames": "^2.2.3",
    "nouislider": "^8.2.1",
    "react-modal": "^0.6.1",
    "react-mq": "github:rmdort/react-mq",
    "react-onclickoutside": "^4.5.0",
    "react-list": "^0.7.12"
  }
}
